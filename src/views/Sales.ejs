                                                            <label for="inputSKU4" class="form-label">SKU</label>
                                                            <input type="text" name="sku" required value=""
                                                                class="form-control" id="inputSKU4"
                                                                placeholder="SKU-CODE">
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label for="inputCategory<%=i%>"
