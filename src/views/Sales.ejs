                            <th>Preço</th>
                            <th>Ações</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% let i=0 ; %>


                            <% i++ %>
                                <tr id="lead-" <% i %> >
                                    <td>
                                        <%= i %>
                                    </td>
                                    <td id="nome-lead">
                                        <div class="lead-img">
                                            <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="black"
                                                class="bi bi-hexagon-half" viewBox="0 0 16 16">
                                                <path
                                                    d="M14 4.577v6.846L8 15V1zM8.5.134a1 1 0 0 0-1 0l-6 3.577a1 1 0 0 0-.5.866v6.846a1 1 0 0 0 .5.866l6 3.577a1 1 0 0 0 1 0l6-3.577a1 1 0 0 0 .5-.866V4.577a1 1 0 0 0-.5-.866z" />
