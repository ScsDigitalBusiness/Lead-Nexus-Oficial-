  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  <script> 
   
   const leadsInDb = <%- JSON.stringify(allLeadsOnDB) %>;
    const process =  <%- JSON.stringify(allProcess) %>;  
    const nameProcess = process.map(prcs=> prcs.process);   
    const leadName = [] 
   
       leadName  = leadsInDb.map(lead=> lead.name)

    
  
    
   
    const ctx3 = document.getElementById("donout");
    new Chart(ctx3, {
      type: "bar",
      data: {
        labels: leadName,
        datasets: [
          {
            label: "Materiais Cadastrados",
            data: [],
            borderWidth: 1,
            backgroundColor: [
              '#5EE95C',
              '#5EE95C',
              '#5EE95C',
              '#5EE95C',
              '#5EE95C',
              '#5EE95C',
              '#5EE95C'
            ],
            borderColor: '#5EE95C',
          },
        ],
